package collecion;

import java.util.*;

public class ProblemStatement_1 {
	public static void main(String[] args) {
		System.out.println("Problem Statement 1:\n");
		List<String> L1 = new ArrayList<String>();//Declaring Array list
		//Adding element to the Array List
		L1.add("Red");
		L1.add("Blue");
		L1.add("Green");
		L1.add("Black");
		System.out.println("Original Color_list = "+L1);
		L1.add(2,"Yellow");//adding new element in the List
		System.out.println("\nNew Color_list  = "+L1);

	}

}
